void main() {
    int x = 1;
    int y = 2;
    int z = 3;
    assert (x < y) && (2 < z);
    assert (x < 2) || (y < 1);
    assert (y < 1) && (z < 2) || (2 < 3);
    assert (x < y) || (z < 2) && (y < 3);
    assert ((x+x) < 1) && ((x+y) < 2) || (2 < 3);
    assert (x < (x+x)) || ((x+(x+x)) < 2) && (y < 3);    
}
